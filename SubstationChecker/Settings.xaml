        <Label Content="COM порт Модема:" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.211,0.577"/>
        <TextBox x:Name="tbArduinoPort" HorizontalAlignment="Left" Height="23" Margin="133,26,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120"/>
